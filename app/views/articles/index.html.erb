<div class="container">
  <h1>All Articles</h1>
  <ul id="articles">
    <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, article_path(article) %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Create new article", new_article_path, class: 'new_article_link' %>
  <%= link_to "See all tags", tags_path, class: 'tags_link' %>

</div>
